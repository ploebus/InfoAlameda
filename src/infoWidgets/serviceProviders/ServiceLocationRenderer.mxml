	<s:Label text="{data.city}" color="0x000000" width="275" height="14" verticalAlign="middle"/>
	<s:Label text="{data.zip}" color="0x000000" width="275" height="14" verticalAlign="middle"/>
	<s:Label text="{data.phone}" color="0x000000" width="275" height="14" verticalAlign="middle"/>
