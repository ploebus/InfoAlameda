				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx" 
				autoDrawBackground="true" label="Open 211">
	<mx:VBox label="Open 211">
		
	
		
	<s:layout>
		<s:VerticalLayout horizontalAlign="center"/>
	</s:layout>
		
	<s:Label text="{data.name}" color="0x000000" x="162" y="10" width="404" height="47" textAlign="center" fontSize="16" verticalAlign="middle"/>
