<?xml version="1.0" encoding="utf-8"?>
<GraphicBorderSkin
		xmlns="http://www.degrafa.com/2007"
		xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			override public function get measuredWidth():Number{
				return 8;
			}
			override public function get measuredHeight():Number{
				return 1;
			}
		]]>
	</fx:Script>
	
	<fills>
		<SolidFill id="mainFill" color="#AAAAAA" alpha="0.8"/>
	</fills>
	<strokes>
		<SolidStroke id="mainStroke" color="#000000" alpha="0"/>
	</strokes>
	<geometry>
		<RoundedRectangle id="track"
						  width="{this.skinWidth}" height="{this.skinHeight}"
						  fill="{this.mainFill}" stroke="{this.mainStroke}"
						  cornerRadius="5"/>
	</geometry>
</GraphicBorderSkin>
